        "driver": "osgar.drivers.audio:Audio",
        "init": {
          "rate": 44100,
          "channels": 1,
          "format": "paInt16",
          "frames_per_buffer": 1024,
          "device_index": null
        }
      },
      "audio_player": {
        "driver": "dtc_audio:DTCAudio",
