{
  "version": 2,
  "robot": {
    "modules": {
      "rc": {
        "driver": "remote:RemoteClient",
        "init": {}
      },
      "to_send": {
          "driver": "osgar.drivers.push:Push",
          "init": {
            "endpoint": "tcp://127.0.0.1:5565"
          }
      },
      "from_robot": {
          "driver": "osgar.drivers.pull:Pull",
              "init": {
              "endpoint": "tcp://0.0.0.0:5566",
              "outputs": ["image", "pose2d"]
          }
      }
    },
    "links": [
      ["rc.cmd", "to_send.cmd"],
      ["from_robot.image", "rc.image"]
    ]
  }
}
