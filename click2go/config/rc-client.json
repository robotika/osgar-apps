{
  "version": 2,
  "robot": {
    "modules": {
      "rc": {
        "driver": "remote:RemoteClient",
        "init": {}
      },
      "to_send": {
          "driver": "osgar.drivers.push:Push",
          "init": {
            "endpoint": "tcp://m03-odroid.local:5565"
          }
      },
      "from_robot": {
          "driver": "osgar.drivers.pull:Pull",
              "init": {
              "endpoint": "tcp://0.0.0.0:5566",
              "outputs": ["color", "pose2d"]
          }
      }
    },
    "links": [["rc.cmd", "to_send.cmd"]]
  }
}
